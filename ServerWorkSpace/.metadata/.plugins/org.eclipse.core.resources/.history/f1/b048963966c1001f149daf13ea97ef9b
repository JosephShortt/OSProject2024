
public class Report {
	
	private String reportType,date,employeeID,status,asigneeID;
	private int reportID;
	
	public Report(String reportType,String date, String employeeID, String status, String asigneeID, int reportID) {
		this.reportType=reportType;
		this.date = date;
		this.employeeID=employeeID;
		this.status = status;
		this.asigneeID=asigneeID;
		this.reportID=reportID;
	}
	
	public String toString()
	{
	    return reportType + "||" + date + "||" + employeeID + "||" + status + "||" + asigneeID + "||" + reportID;
	}
}
